# Environment variables for Fabric CI/CD
# Copy this file to .env and fill in your values

# Required: Fabric authentication
FABRIC_TOKEN=your_fabric_token_here
TENANT_ID=your_azure_tenant_id_here

# Optional: GitHub integration (for Git connections)
GITHUB_TOKEN=your_github_token_here

# Optional: Azure authentication (for advanced scenarios)
AZURE_CLIENT_ID=your_azure_client_id_here
AZURE_CLIENT_SECRET=your_azure_client_secret_here

# Optional: Organization-specific settings
ORGANIZATION_NAME=your_organization_name
DEFAULT_CAPACITY_ID=your_default_fabric_capacity_id

# Development settings
DEV_CAPACITY_ID=F2
STAGING_CAPACITY_ID=F32
PROD_CAPACITY_ID=F64

# Logging
LOG_LEVEL=INFO
AUDIT_LOG_DIR=audit_logs



# Mandatory Security Principals (Object IDs)
EU_M365_SEC_EMEA_AZ_PBI_IT_ADMIN_ID=
EU_M365_SEC_EMEA_AZ_PBI_EDP_SUPPORT_ID=

# Dev Environment Principals
DEV_ADMIN_EMAIL=
DEV_ADMIN_OBJECT_ID=
